{
  "name": "frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build && tsc",
    "check": "prettier format && eslint --fix",
    "dev": "vite --port 5713",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:openapi": "openapi-typescript ../backend/restapi-macroservice/docs/openapi/restapi-macroservice.openapi.json -o src/lib/api/types.ts",
    "lint": "eslint",
    "serve": "vite preview",
    "start": "vite --port 5713",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
